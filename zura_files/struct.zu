const Hobbies := enum {
    Cooking,
    Programming,
    Drawing,
    Reading,
}; 

const Person := struct {
    name: str,
    age: int!,
    
    greet := fn (self: *Person) void {
        @output(1, "Hello, my name is ", self.name, " and I am ", self.age, " years old.\n");
    }; 
};

const main := fn () int! {
  have John: Person = {
    name: "John",
    age: 25
  };

  John.greet(); # 'self' not included here

  return 0;
};
