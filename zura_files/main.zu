const main := fn () int! {
  have x: int! = @getArgc();
  if (x < 2) {
    @outputln(1, "You did not pass any inputs into Zura.");
    return 1;
  }
  @outputln(1, "You passed in ", x, " inputs into Zura.");
  have y: *[]str = @getArgv();
  loop (i = 0; i < x) : (i++) {
    @outputln(1, "Input ", i, ": ", y&[i]);
  };
  return 0;
};