const InnerTest := struct {
  x: int,
  y: int,
};

const Test := struct {
  z: int,
  a: int,
  b: int,
  c: InnerTest,
};

const main := fn () int {
  have a: Test = {
    z: 0,
    a: 1,
    b: 2,
    c: {
      x: 3,
      y: 4
    }
  };

  a.c = {
    x: 5,
    y: 6
  };

  a.z = 7;

  return a.a;
};