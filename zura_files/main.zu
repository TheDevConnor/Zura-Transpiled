const Weekday := enum {
  Monday,
  Tuesday,
  Wednesday,
  Thursday,
  Friday,
  Saturday,
  Sunday
}; 

const main := fn () int {
  have x: *int;
  @output("Size of x: ", @sizeof(x), "\n"); # 4

  have y: str;
  @output("Size of y: ", @sizeof(y), "\n"); # 8

  @output("Size of z: ", @sizeof(Weekday), "\n"); # 4

  return 0;
};