const main := fn () int! {
  have x: int! = @getArgc();
  have y: *[]str = @getArgv();

  if (x < 2) {
    @outputln(1, "You did not pass any inputs into Zura.");
    return 1;
  }

  @outputln(1, "You passed in ", x, " inputs into Zura.");
  # NOTE: Fixed the strcmp but now when we do i < x we segfault because we are going out of bounds it seems like
  loop (i = 0; i < 2) : (i++) {
    if (@strcmp(y&[i], "--build") == 0) {
      @outputln(1, "IN HERE -> ", y&[i]);
    }
  };

  return 0;
};

