const main := fn () int {
    loop(i = 1; i <= 3) : (i = i + 1) {
        dis("Outer loop: i \n");
        loop(j = 1; j <= 5) : (j = j + 1) {
            if (j == 3) {
                dis("Break inner loop for j = 3\n");
                break;
            }
            dis("Inner loop: j \n");
        }
    }
    dis("End of main\n");
    return 0;
};