const test2 := struct {
  x: int;
  y: int;
};

const test := struct {
  x: float;
  y: test2;
};

const main := fn () int {
  # I also tested on regular, flat structs with float members
  # and it works fine. It works now!
  have e: test = {
    x: 1.0,
    y: {
      x: 1,
      y: 2
    }
  };

  have y: float = e.x;
  return @cast<int>(y);
};