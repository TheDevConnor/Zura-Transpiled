const Hobbies := enum {
  Drawing,
  Cooking,
  Programming,
  Reading,
};

const main := fn () int {
  have x: Hobbies = Hobbies.Programming;
  match (x) {
    case Hobbies.Drawing -> {
      return 1;
    }
    case Hobbies.Cooking -> {
      return 2;
    }
    case Hobbies.Programming -> {
      return 3;
    }
    case Hobbies.Reading -> {
      return 4;
    }
    default -> {
      return 0;
    }
  }
  return 0;
};